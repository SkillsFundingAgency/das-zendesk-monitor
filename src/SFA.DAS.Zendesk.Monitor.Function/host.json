{
  "version": "2.0",
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Trace"
    }
  },
  "extensions": {
    "http": {
      "hsts": {
        "isEnabled": true,
        "maxAge": "30"
      },
      "customHeaders": {
        "Content-Security-Policy": "default-src 'none';",
        "X-Frame-Options": "DENY",
        "X-Content-Type-Options": "nosniff",
        "X-XSS-Protection":  0
      }
    }
  }
}